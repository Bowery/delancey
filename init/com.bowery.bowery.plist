<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.bowery.bowery</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/bowery-agent</string>
    </array>
    <key>KeepAlive</key>
    <true/>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardErrorPath</key><string>/Library/Logs/bowery-agent-err.log</string>
    <key>StandardOutPath</key><string>/Library/Logs/bowery-agent.log</string>
  </dict>
</plist>
